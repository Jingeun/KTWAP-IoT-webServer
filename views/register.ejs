<div class="content-wrap">
    <!-- main page content. the place to put widgets in. usually consists of .row > .col-md-* > .widget.  -->
    <main id="content" class="content" role="main">
        <ol class="breadcrumb">
            <li>Sensor register</li>
            
        </ol>
        <h1 class="page-title">센서 등록</h1>
                <section class="widget">
                    <header>
                        <h4>
                            <small>정보 입력</small>
                        </h4>
                        <div class="widget-controls">
                        </div>
                    </header>
                    <div class="widget-body">
                        <form id="validation-form" class="form-horizontal form-label-left" method="post"
                              data-parsley-priority-enabled="false"
                              novalidate="novalidate" action='/register/execute' >
                            <fieldset>
                                    <div class="form-group">
                                        <label class="control-label col-sm-3" for="basic">장비 번호</label>
                                        <div class="col-sm-9">
                                            <input type="text" id="dev_serial" name="dev_serial" class="form-control"
                                                   required="required" placeholder="장비 번호를 입력하세요">
                                        </div>
                                    </div>
									<div class="form-group">
                                        <label class="control-label col-sm-3" for="basic">이름</label>
                                        <div class="col-sm-9">
                                            <input type="text" id="place_name" name="place_name" class="form-control"
                                                   required="required" placeholder="장소이름을 입력하세요">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-sm-3" for="basic-change">
                                            위치
                                        </label>
                                        <div class="col-sm-3">
                                            <input type="text" id="address1" name="address1" class="form-control"
                                                   required="required" placeholder="시,군,구">
                                        </div>
										<div class="col-sm-3">
                                            <input type="text" id="address2" name="address2" class="form-control"
                                                   required="required" placeholder="읍,면,동">
                                        </div>
                                        <div class="col-sm-3">
                                            <input type="text" id="address3" name="address3" class="form-control"
                                                   required="required" placeholder="리">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="col-sm-9 col-sm-offset-3">
                                            <input type="text" id="address_detail" name="address_detail" class="form-control"
                                                   required="required" placeholder="상세주소">
                                        </div>
                                    </div>

                            </fieldset>
                           <!--  <fieldset>
                                <legend>
                                    <span class="label label-warning  text-gray-dark mr-xs">
                                        특이사항
                                    </span>    
                                </legend>
                            </fieldset>
                            <fieldset>
                                    <div class="form-group">
                                        <label class="control-label col-sm-3" for="password">
                                         비고
                                        </label>
                                        <div class="col-sm-8">
                                            <input type="text" id="basic" name="basic" class="form-control"
                                                   required="required">
                                        </div>
                                    </div>
                            </fieldset> -->
                            <div class="form-actions">
                                <button type="submit" class="btn btn-danger btn-rounded pull-right">등록</button>
								
                                <a type="button" href='/main' class="btn btn-default btn-rounded">취소</a>
                            </div>
                        </form>
                    </div>
                </section>       
    </main>

<!-- The Loader. Is shown when pjax happens -->
